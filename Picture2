
public class Picture2 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i = 1; i <= 5; i++){				//控制行数
			for(int n = i; n <= 5; n++){			//控制每行空格
				System.out.print("  ");
			}				
			for(int j = 1; j <= 10; j++){			//控制每行输出“*”的个数
				System.out.print("*");
			}
			System.out.println();					//换行
		}
	}

}
